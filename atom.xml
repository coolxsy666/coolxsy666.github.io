|&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;tags&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;categories:&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;/categories/&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;||&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;th&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;archives:&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;/archives/&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;||&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;archive&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;comment&quot;&gt;#schedule: /schedule/ || calendar&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;sitemap:&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;/sitemap.xml&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;||&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;sitemap&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;commonweal:&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;/404/&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;||&lt;/span&gt; &lt;span class=&quot;string&quot;&gt;heartbeat&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;attr&quot;&gt;menu_settings:&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;icons:&lt;/span&gt; &lt;span class=&quot;literal&quot;&gt;true&lt;/span&gt;   &lt;span class=&quot;comment&quot;&gt;# 显示图标&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;  &lt;span class=&quot;attr&quot;&gt;badges:&lt;/span&gt; &lt;span class=&quot;literal&quot;&gt;true&lt;/span&gt;  &lt;span class=&quot;comment&quot;&gt;# 显示统计信息&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
